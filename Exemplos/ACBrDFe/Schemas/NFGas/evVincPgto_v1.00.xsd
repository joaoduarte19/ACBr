<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.portalfiscal.inf.br/nfgas" xmlns:ds="http://www.w3.org/2000/09/xmldsig#" targetNamespace="http://www.portalfiscal.inf.br/nfgas" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:include schemaLocation="eventoNFGasTiposBasico_v1.00.xsd"/>
	<xs:include schemaLocation="DFeTiposBasicos_v1.00.xsd"/>
	<xs:element name="evVincPgto">
		<xs:annotation>
			<xs:documentation>Schema XML de validação do evento de vinculação da transação de pagamento com o DFe  - 110300</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="descEvento">
					<xs:annotation>
						<xs:documentation>Descrição do Evento - “Vinculação Pagamento”</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:whiteSpace value="preserve"/>
							<xs:enumeration value="Vinculação do Pagamento"/>
							<xs:enumeration value="Vinculacao do Pagamento"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="nProt" type="TProt">
					<xs:annotation>
						<xs:documentation>Número do Protocolo de autorização do DFe</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="pgto" type="TPagamentoRTC">
					<xs:annotation>
						<xs:documentation>Vinuclação com o pagamento</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
		<xs:unique name="pk_nPag">
			<xs:selector xpath="./*"/>
			<xs:field xpath="@nPag"/>
		</xs:unique>
		<xs:unique name="pk_idTransacao">
			<xs:selector xpath="./*"/>
			<xs:field xpath="@idTransacao"/>
		</xs:unique>
	</xs:element>
